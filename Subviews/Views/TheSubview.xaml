<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Subviews.Views.TheSubview"
	     x:Name="This">
    <ContentView.Content>
        <StackLayout>
            <Label BindingContext="{x:Reference This}" Text="{Binding Path=Title}" />
            <Entry x:Name="TheEntry" BindingContext="{x:Reference This}" Text="{Binding Path=Text}" />
        </StackLayout>
    </ContentView.Content>
</ContentView>
